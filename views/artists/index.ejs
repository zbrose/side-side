<ul>
    
    <% artists.forEach(artist => { %>
        
        <div>
            <h3><a href="/artists/<%=artist.id%>"><%= artist.title %></a> </h3>
        </div>
       
        <a href="/results/<%=artist.id%>"><img src="<%=artist.cover_image %>" width="300px" alt="cover image"></a>
        
        <div>
             <a href="/results/<%=artist.id%>"><button>See All Albums</button></a>
        </div>
        
    <% })%>

</ul>
