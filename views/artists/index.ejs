<ul>
    
    <% artists.forEach(artist => { %>
        <h4><%= artist.title %> </h4>
        <div><img src="<%=artist.cover_image %>" width="300px" alt="cover image"></div> 
        <a href="/results/<%=artist.id%>"><button type="button" class="btn btn-success">See Albums</button></a>
        <a href="/artists/<%=artist.id%>"><button type="button" class="btn btn-success">See Profile</button></a>
        
    <% })%>

</ul>
